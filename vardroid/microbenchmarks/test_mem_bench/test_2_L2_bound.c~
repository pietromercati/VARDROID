		t1 = get_cyclecount();
		//printf("MEM\t%d\t%d\t%d\n", t1-t0, sizeof(int)*SIZE, STRIDE);
		n += step; sum_t += (t1 - t0);
	}

	printf("MEMAVG\t%d\t%d\t%d\n", (int)(sum_t / n), sizeof(int)*SIZE, STRIDE);

	free(v);
}

void memlat_test(int iteration) {
	const int MAX_STRIDE = 128;
	const int MAX_SIZE = 1024*1024;
	int i,j;
	for (j = 2; j <= MAX_SIZE; j *= 2) {
		for (i = 2; i <= MAX_STRIDE; i += 2) {
			mem_accesstime(iteration, i, j);
		}
	}
}







int main(int argc, char ** argv){

int main(int argc, char *argv[]) {


	int iteration = atoi(argv[1]);

	overhead_test(iteration); 

	return 0;
} 




